function char_mood_calc() {
	t = get_mood_arg();
	x = t * 6.2831853072;
	x2 = x*x;
	x4 = x2*x2;
	x6 = x4*x2;
	x8 = x4*x4;
	x10 = x8*x2;
	x12 = x10*x2;
	x14 = x12*x2;
	x16 = x8*x8;

	y = 1.0 - 0.25*x2 + 0.02083333*x4 - 0.00069444*x6 + 0.0000124007936508*x8 - 0.000000137787*x10 + 0.000000001044*x12 - 0.00000000000574*x14 + 0.0000000000000239*x16;
	return y;
}
